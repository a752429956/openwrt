config wifi-device 'radio0'
	option type 'none'
	option path 'platform/98010c00.sdio/mmc_host/mmc0/mmc0:0001/mmc0:0001:1'
	option hwmode '11n'
	option channel 'auto'

config wifi-iface 'default_radio0'
	option device 'radio0'
	option mode 'sta'
	option ifname 'wlan0'
	option short_preamble '0'
	option disassoc_low_ack '0'
	option encryption 'none'
	option network 'wlan'
