config interface loopback
	option ifname	lo
	option proto	static
	option ipaddr	127.0.0.1
	option netmask	255.0.0.0
	option dns '223.5.5.5 8.8.8.8'

config interface 'lan'
	option ifname 'eth0'
	option proto 'static'
	option ipaddr '192.168.1.1'
	option netmask '255.255.255.0'

config interface 'lan_dhcp'
	option proto 'dhcp'
	option ifname 'eth0'
	option auto '0'

config interface 'wlan0'
	option proto 'dhcp'
	option ifname 'wlan0'
	option auto '0'
